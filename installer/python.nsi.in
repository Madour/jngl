XPStyle On
SetCompressor lzma

Name "JNGL for Python 3.4"

OutFile "JNGL @VERSION@ (Python 3.4).exe"

InstallDir "C:\Python34\Lib\site-packages"

Page directory
Page instfiles

LoadLanguageFile "${NSISDIR}\Contrib\Language files\English.nlf"

Section ""

SetOutPath $INSTDIR
File /oname=jngl.pyd ..\visualcpp\Release\Python.dll
File ..\visualcpp\Release\jngl.dll
File ..\visualcpp\packages\libogg.redist.1.3.0.1\build\native\bin\v110\Win32\Release\dynamic\libogg.dll

SectionEnd
